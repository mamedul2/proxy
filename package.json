{
  "name": "nodejs-proxy-server",
  "version": "1.0.0",
  "description": "A Node.js proxy server that handles all HTTP methods and CORS",
  "author": "MAMEDUL ISLAM",
  "main": "proxy.js",
  "builds": [
    {
      "src": "proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "proxy.js"
    }
  ],
  "scripts": {
    "start": "node proxy.js",
    "dev": "nodemon proxy.js"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "license": "MIT"
}